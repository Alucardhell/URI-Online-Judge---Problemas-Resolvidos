#include<bits/stdc++.h>
using namespace std;

int main()
{
	int i, p = 0;
	float x[6], tot = 0;

	for (i = 0; i < 6; i++)
	{

		cin >> x[i];

	}

	for (i = 0; i < 6; i++)
	{

		if (x[i] > 0)
		{
			p++;

			tot = tot + x[i];

		}
	}
	printf ("%d valores positivos\n", p);
	printf ("%0.1f\n", tot / p);


}
