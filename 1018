#include<bits/stdc++.h>
using namespace std;
int main()
{
	int n, n100 = 0, n50 = 0, n20 = 0, n10 = 0, n5 = 0, n2 = 0, n1 = 0, t;

	cin >> n;
	t = n;

	while (n >= 100)
	{
		n100++;
		n = n - 100;
	}

	while (n >= 50)
	{
		n50++;
		n = n - 50;
	}

	while (n >= 20)
	{
		n20++;
		n = n - 20;
	}
	while (n >= 10)
	{
		n10++;
		n = n - 10;
	}
	while (n >= 5)
	{
		n5++;
		n = n - 5;
	}
	while (n >= 2)
	{
		n2++;
		n = n - 2;
	}
	while (n >= 1)
	{
		n1++;
		n = n - 1;
	}

	cout << t << endl;
	printf ("%d nota(s) de R$ 100,00\n", n100);
	printf ("%d nota(s) de R$ 50,00\n", n50);
	printf ("%d nota(s) de R$ 20,00\n", n20);
	printf ("%d nota(s) de R$ 10,00\n", n10);
	printf ("%d nota(s) de R$ 5,00\n", n5);
	printf ("%d nota(s) de R$ 2,00\n", n2);
	printf ("%d nota(s) de R$ 1,00\n", n1);

	return 0;


}
