#include<bits/stdc++.h>
using namespace std;
int main()
{
	float n = 0;
	int n100 = 0, n50 = 0, n20 = 0, n10 = 0, n5 = 0, n2 = 0, m11 = 0, m50 = 0, m25 = 0, m10 = 0, m5 = 0, m1 = 0;


	cin >> n;
	while (n >= 100)
	{
		n100++;
		n = n - 100;
	}

	while (n >= 50)
	{
		n50++;
		n = n - 50;
	}

	while (n >= 20)
	{
		n20++;
		n = n - 20;
	}

	while (n >= 10)
	{
		n10++;
		n = n - 10;
	}

	while (n >= 5)
	{
		n5++;
		n = n - 5;
	}
	while (n >= 2)
	{
		n2++;
		n = n - 2;
	}

	while (n >= 1)
	{
		m11++;
		n = n - 1;
	}

	while (n >= 0.50)
	{
		m50++;
		n = n - 0.50;
	}

	while (n >= 0.25)
	{
		m25++;
		n = n - 0.25;
	}

	while (n >= 0.10)
	{
		m10++;
		n = n - 0.10;
	}

	while (n >= 0.05)
	{
		m5++;
		n = n - 0.05;
	}

	while (n > 0.001)
	{
		m1++;
		n = n - 0.01;
	}

	cout << "NOTAS:" << endl;
	printf ("%d nota(s) de R$ 100.00\n", n100);
	printf ("%d nota(s) de R$ 50.00\n", n50);
	printf ("%d nota(s) de R$ 20.00\n", n20);
	printf ("%d nota(s) de R$ 10.00\n", n10);
	printf ("%d nota(s) de R$ 5.00\n", n5);
	printf ("%d nota(s) de R$ 2.00\n", n2);
	cout << "MOEDAS:" << endl;
	printf ("%d moeda(s) de R$ 1.00\n", m11);
	printf ("%d moeda(s) de R$ 0.50\n", m50);
	printf ("%d moeda(s) de R$ 0.25\n", m25);
	printf ("%d moeda(s) de R$ 0.10\n", m10);
	printf ("%d moeda(s) de R$ 0.05\n", m5);
	printf ("%d moeda(s) de R$ 0.01\n", m1);

	return 0;
}
