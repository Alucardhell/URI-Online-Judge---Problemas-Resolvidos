#include<bits/stdc++.h>
#include <string.h>
using namespace std;

int main ()
{


	int n1, n2;

	cin >> n1 >> n2;

	if (n1 == n2)
	{

		printf ("O JOGO DUROU 24 HORA(S)\n");
	}
	else
	{
		if (n1 > n2)
		{

			printf ("O JOGO DUROU %d HORA(S)\n", (n2 + 24) - n1);
		}
		if (n1 < n2)
		{

			printf ("O JOGO DUROU %d HORA(S)\n", n2 - n1);
		}
	}


}
